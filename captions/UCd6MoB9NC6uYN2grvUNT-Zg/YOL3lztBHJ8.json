[
  {
    "text": "一番驚いたのは彼新入社員でもできること",
    "start": "719",
    "end": "4020"
  },
  {
    "text": "がすごいなぁということに驚きましたん",
    "start": "4020",
    "end": "7490"
  },
  {
    "text": "ん",
    "start": "7490",
    "end": "9490"
  },
  {
    "text": "うううう",
    "start": "9670",
    "end": "12570"
  },
  {
    "text": "アマゾンウェブサービスで働いております",
    "start": "13930",
    "end": "16880"
  },
  {
    "text": "森川ですクラウドサポートエンジニア",
    "start": "16880",
    "end": "19039"
  },
  {
    "text": "チームで働いております aws サービス",
    "start": "19039",
    "end": "22220"
  },
  {
    "text": "を使いなってお客様からのお問い合わせに",
    "start": "22220",
    "end": "24710"
  },
  {
    "text": "答えるという仕事になります最初 aws",
    "start": "24710",
    "end": "27710"
  },
  {
    "text": "全然知らなかったので身近にも感じなかっ",
    "start": "27710",
    "end": "31340"
  },
  {
    "text": "たというかすごい遠い存在に感じていたん",
    "start": "31340",
    "end": "34620"
  },
  {
    "text": "ですけれども調べてみるとすごい身近な",
    "start": "34620",
    "end": "37500"
  },
  {
    "text": "サービスで使われていることが分かって",
    "start": "37500",
    "end": "40440"
  },
  {
    "text": "なんかこんな会社があるんだと思って",
    "start": "40440",
    "end": "42510"
  },
  {
    "text": "インターンに参加してみることにしました",
    "start": "42510",
    "end": "45030"
  },
  {
    "text": "すごい働いている方々が素敵な方たち",
    "start": "45030",
    "end": "48239"
  },
  {
    "text": "ばかりで気さくに話していただいたり",
    "start": "48239",
    "end": "51180"
  },
  {
    "text": "わかんない件とかもすごく屋さんセク教え",
    "start": "51180",
    "end": "54100"
  },
  {
    "text": "てくださってすごい素敵な方が働いている",
    "start": "54100",
    "end": "57340"
  },
  {
    "text": "んだなあというふうに感じました一番驚い",
    "start": "57340",
    "end": "59740"
  },
  {
    "text": "たのは彼新入社員でもできることがすごい",
    "start": "59740",
    "end": "63010"
  },
  {
    "text": "なぁということに驚きましたポロッとなん",
    "start": "63010",
    "end": "65620"
  },
  {
    "text": "かこれこういう風にしたらいいですよねっ",
    "start": "65620",
    "end": "67810"
  },
  {
    "text": "ていうふうに行った事があるんですけど",
    "start": "67810",
    "end": "69670"
  },
  {
    "text": "剃ったらマネージャーさんがそれを拾って",
    "start": "69670",
    "end": "71710"
  },
  {
    "text": "いただいて実現に向けて8それどういう",
    "start": "71710",
    "end": "74719"
  },
  {
    "text": "ことでいうふうに詳しく聞いていただいて",
    "start": "74719",
    "end": "78090"
  },
  {
    "text": "で実際にそれが実現したということがあっ",
    "start": "78090",
    "end": "80670"
  },
  {
    "text": "たのでああなんか新入社員の一意見でも",
    "start": "80670",
    "end": "83910"
  },
  {
    "text": "こんなに一つの意見として尊重してもらえ",
    "start": "83910",
    "end": "86880"
  },
  {
    "text": "てそれを実現に持って行ってくださるって",
    "start": "86880",
    "end": "89220"
  },
  {
    "text": "いうのはすごい",
    "start": "89220",
    "end": "91030"
  },
  {
    "text": "感動しました私は aw 学生の時に",
    "start": "91030",
    "end": "94450"
  },
  {
    "text": "aws のサービスに触ったことがなかっ",
    "start": "94450",
    "end": "96790"
  },
  {
    "text": "たのでタッチアップしていくというところ",
    "start": "96790",
    "end": "99010"
  },
  {
    "text": "で結構苦労したんですねそういった自分が",
    "start": "99010",
    "end": "102040"
  },
  {
    "text": "苦しい時にメンターさんになんでも聞いて",
    "start": "102040",
    "end": "105400"
  },
  {
    "text": "っていう温かいことをかけていただいたり",
    "start": "105400",
    "end": "108280"
  },
  {
    "text": "マネージャーさんには今の私にできること",
    "start": "108280",
    "end": "110649"
  },
  {
    "text": "をなにかっていうところを考えて目標を",
    "start": "110649",
    "end": "113090"
  },
  {
    "text": "設定して頂いてそれに対する目標達成に",
    "start": "113090",
    "end": "116600"
  },
  {
    "text": "対するサポートっていうのを最大限にして",
    "start": "116600",
    "end": "118670"
  },
  {
    "text": "いただいたりしました女性端から何何って",
    "start": "118670",
    "end": "121549"
  },
  {
    "text": "いうことが全くないので性別に起因する",
    "start": "121549",
    "end": "124250"
  },
  {
    "text": "ような働きやすさだったりニックさって",
    "start": "124250",
    "end": "126799"
  },
  {
    "text": "いうのはないと思います年代が上だから何",
    "start": "126799",
    "end": "130459"
  },
  {
    "text": "点でしょう雷っていうわけでもなくてなん",
    "start": "130459",
    "end": "133590"
  },
  {
    "text": "というか対等に話し合いができる感じなの",
    "start": "133590",
    "end": "136769"
  },
  {
    "text": "でそういった面でも働きやすかっていうの",
    "start": "136769",
    "end": "139560"
  },
  {
    "text": "は感じています aws という会社を",
    "start": "139560",
    "end": "141900"
  },
  {
    "text": "もしかしたらこの子ない方もいらっしゃる",
    "start": "141900",
    "end": "143879"
  },
  {
    "text": "かとは思うんですが",
    "start": "143879",
    "end": "145560"
  },
  {
    "text": "実は皆周りのゲームであったり",
    "start": "145560",
    "end": "147750"
  },
  {
    "text": "アプリケーション裏側であったり ws の",
    "start": "147750",
    "end": "150450"
  },
  {
    "text": "サービスが動いているということがあり",
    "start": "150450",
    "end": "152560"
  },
  {
    "text": "ますぜひ興味を持っていただけたら幸い",
    "start": "152560",
    "end": "155230"
  },
  {
    "text": "です",
    "start": "155230",
    "end": "156340"
  },
  {
    "text": "一緒に働ける日を楽しみにしております",
    "start": "156340",
    "end": "159470"
  },
  {
    "text": "[音楽]",
    "start": "159470",
    "end": "164110"
  }
]